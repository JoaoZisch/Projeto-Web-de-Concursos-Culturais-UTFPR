<div class="py-5 text-center text-white">
        <h2>Concursos</h2>
</div>

<div class="container2">
{{#each concursos}}
  <div class="card">
    <div class="card__header">
       <img src="../uploads/{{id}}0690.jpg" alt="card__image" class="card__image" width="600">
    </div>
    <div class="card__body">
      <h4>{{nome}}</h4>
      <p>{{descricao}}</p>
    </div>
    <div class="card__footer">
      <form action="/infoConcurso/{{nome}}" method="get">  
            <button class="button" style="vertical-align:middle" type="submit" value="Visualisar" ><span>Visualizar </span></button>
      </form>
    </div>
  </div>
{{/each}}
</div>

